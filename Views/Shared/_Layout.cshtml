    <!-------------------------code pour js ----------------------------->

    <script src="~/js/admine.js"></script>

    <script src="~/js/clock.js"></script>
    <script src="~/lib/bootstrap/bootsrap-js/bootstrap.min.js"></script>
    <script src="~/lib/bootstrap/bootsrap-js/bootstrap.bundle.js"></script>
    
    
